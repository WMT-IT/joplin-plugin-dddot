    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-jest": "^27.5.1",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^27.5.1",
